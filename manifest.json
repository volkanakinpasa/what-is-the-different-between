{
  "manifest_version": 3,
  "name": "Google diff search",
  "short_name": "diff",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "2",
  "omnibox": {
    "keyword": "diff"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "What is the difference between ... and ...",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "options_page": "options.html"
}
