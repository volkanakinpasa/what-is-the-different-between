{
  "manifest_version": 2,
  "name": "What is the difference between",
  "short_name": "diff",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.2",
  "permissions": [
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "omnibox": {
    "keyword": "diff"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "What is the difference between ... and ...",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }, "options_page": "options.html"
}